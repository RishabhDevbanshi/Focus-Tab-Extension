{
    "manifest_version": 2,
    "name": "Focus Tab",
    "description": "launches a new focus tab which greets you, shows time and your focus for today.",
    "version": "1.0.0",
    "icons": {
        "128": "assets/128logo.png"
    },
    "browser_action": {
        "default_icon": "assets/logo.png",
        "default_popup": "popup.html"
    },
    "permissions": [
        "activeTab"
    ]
}